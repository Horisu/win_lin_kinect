<Window x:Class="KinectRgbdInteractionServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KinectRgbdInteractionServer"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525"
        Loaded="MainWindow_Loaded"
        Closing="MainWindow_Closing">
    <Grid>
        <Canvas x:Name="canvas">
            <TextBlock x:Name="status" Width="517"></TextBlock>
            <TextBlock x:Name="audio" Canvas.Top="16" Width="517"></TextBlock>
            <TextBlock x:Name="skeletons" Canvas.Top="32" Width="517"></TextBlock>
            <TextBlock x:Name="faces" Canvas.Top="48" Width="517"></TextBlock>
            <TextBlock x:Name="speakers" Canvas.Top="64" Width="517"></TextBlock>
            <TextBlock x:Name="recognized" Canvas.Top="80" Width="517"></TextBlock>
            <TextBlock x:Name="dbgTimeElapsed" Canvas.Top="96" Width="517"></TextBlock>
            <TextBlock x:Name="dbgFailedFrames" Canvas.Top="112" Width="517"></TextBlock>
            <TextBlock x:Name="dbgDepthTime" Canvas.Top="128" Width="517"></TextBlock>
            <TextBlock x:Name="dbgColorTime" Canvas.Top="144" Width="517"></TextBlock>
        </Canvas>
    </Grid>
</Window>
